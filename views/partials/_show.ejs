<% for(var i = 0; i < usersList.length; i++){ %>
<tr>
    <td><%= i+1 %></td>
    <td><%= usersList[i].name %></td>
    <td><%= usersList[i].email %></td>
    <td><%= usersList[i].gender %></td>
    <td><%= usersList[i].status %></td>
    <td>
        <a href="/update-user?id=<%= usersList[i]._id %>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a href="#" class="btn border-shadow delete" id="<%= usersList[i]._id %>">
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>